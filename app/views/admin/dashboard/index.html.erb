<h2>All Users</h2>
<ul>
<% @users.each do |user| %>
<li>
<%= link_to user.email, admin_user_profile_path(user) %>
      | <%= link_to "Delete User", admin_delete_user_path(user), method: :delete, data: { confirm: "Are you sure?" } %>
</li>
<% end %>
</ul>

<%= link_to "Logout", destroy_admin_session_path , method: :delete, data: { turbo: false } %>
